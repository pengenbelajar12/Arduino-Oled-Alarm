// fungsi alarm 
void alarm() {
  timeClient.update();
  int Hour = timeClient.getHours();
  int Minute = timeClient.getMinutes();
  int Second = timeClient.getSeconds();

  // masuk ke menu 
  if(digitalRead(btnMenu) == LOW){
     setmenu++;
     digitalWrite(buzzer,HIGH);
     delay(50);
     digitalWrite(buzzer,LOW);
     Serial.print(setmenu);
    if(setmenu >= 3 ) setmenu = 0;
    delay(300);
  }
  switch (setmenu){
  // set alaarm 1 = setting jam dan 2 = stting menit 
  case 1:
    if(digitalRead(btnInc) == LOW){ digitalWrite(buzzer,HIGH); delay(50); digitalWrite(buzzer,LOW); alarmjam = (alarmjam + 1)% 24; delay(200);}
    if(digitalRead(btnDec) == LOW){ digitalWrite(buzzer,HIGH); delay(50); digitalWrite(buzzer,LOW); alarmjam = (alarmjam - 1 + 24)% 24; delay(200);}
    break;
  case 2:
    if(digitalRead(btnInc) == LOW){ digitalWrite(buzzer,HIGH); delay(50); digitalWrite(buzzer,LOW); alarmmenit = (alarmmenit + 1)% 60; delay(200);}
    if(digitalRead(btnDec) == LOW){ digitalWrite(buzzer,HIGH); delay(50); digitalWrite(buzzer,LOW); alarmmenit = (alarmmenit - 1 + 60)% 60; delay(200);}
    break;
  }

  // setting alarm
  int akhirtekan = HIGH;
  if(digitalRead(btnSelect) == LOW && akhirtekan == HIGH){
    digitalWrite(buzzer,HIGH);
    delay(50); 
    digitalWrite(buzzer,LOW);
    setalarm = !setalarm;
    delay(300);
  }
  akhirtekan = digitalRead(btnSelect);

  // nyocoke jam ro alrm 
  if(setalarm && Hour == alarmjam && Minute == alarmmenit && Second == 0){
    setbuzzer  = true;
  }
  else if (Hour == alarmjam && Minute > alarmmenit ){
    setbuzzer = false;
  }
  
  // setting  buzzer alarm
  if(setbuzzer){
    digitalWrite(buzzer,HIGH);
    delay(100);
    digitalWrite(buzzer,LOW);
  }
  else{
    digitalWrite(buzzer,LOW);
  }

  // tampilkan di display
  display.clearDisplay();
  display.setTextSize(2);
  display.setTextColor(SSD1306_WHITE);

  // tampilkan alarm 
  display.setTextSize(1);
  display.setCursor(0, 35);
  display.print("Alarm: ");
  if (alarmjam < 10) display.print("0");
  display.print(alarmjam);
  display.print(":");
  if (alarmmenit < 10) display.print("0");
  display.print(alarmmenit);

  if(setalarm) display.print("  ON");
  else display.print("  OFF");

  // masuk mode apa?
  display.setCursor(0,50);
  if(setmenu == 0) display.print("OK ga kik? "); Serial.println("info 1");
  if(setmenu == 1) display.print("Setting jam"); Serial.println("info 2");
  if(setmenu == 2) display.print("Setting menit"); Serial.println("info 3");

  display.display();
}

// fungsi tampilin jam
void jam() {
  timeClient.update();
  int Hour = timeClient.getHours();
  int Minute = timeClient.getMinutes();
  int Second = timeClient.getSeconds();

    // nyocoke jam ro alrm 
  if(setalarm && Hour == alarmjam && Minute == alarmmenit && Second == 0){
    setbuzzer  = true;
  }
  else if (Hour == alarmjam && Minute > alarmmenit ){
    setbuzzer = false;
  }
  
  // setting  buzzer alarm
  if(setbuzzer){
    digitalWrite(buzzer,HIGH);
    delay(100);
    digitalWrite(buzzer,LOW);
  }
  else{
    digitalWrite(buzzer,LOW);
  }

  display.clearDisplay();
  display.setTextSize(2);
